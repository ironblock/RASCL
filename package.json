{
  "name": "rascl",
  "version": "0.1.0",
  "description": "Redux API State Caching Layer",
  "keywords": [
    "redux",
    "typescript"
  ],
  "type": "module",
  "main": "dist/rascl.umd.js",
  "module": "dist/rascl.es5.js",
  "typings": "rascl.d.ts",
  "files": [
    "dist"
  ],
  "author": "Corey Vixie",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ironblock/RASCL.git"
  },
  "bugs": {
    "url": "https://github.com/ironblock/RASCL/issues"
  },
  "homepage": "https://github.com/ironblock/RASCL#readme",
  "license": "BSD 2-Clause",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint-fix": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {},
  "dependencies": {
    "@types/eslint": "^7.2.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/prettier": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "prettier": "^2.2.1",
    "prompt": "^1.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "typescript": "^4.1"
  }
}
